<testcases>
<testcase id="eng.disamb.1" type="pertinent">
  <call-parameters>
      <param key="text" value="Birds of a feather flee together to escape."/>
      <param key="language" value="eng"/>
      <list key="pipelines">
        <item value="indexer"/>
        <item value="query"/>
      </list>
    </call-parameters>
  <expl>prep infinitif = configuration interdite dans la matrice.</expl>
  <test id="eng.disamb.1.1" trace=".default.xml"
    comment="Apres analyse morphologique, verifier que le mot Birds a la propriete NC "
    left="XPATH#//data_structure/vertex[token/position=1]/data//property/p[@prop='MACRO']/@val"
    operator="contains"
    right="L_NC" />
  <test id="eng.disamb.1.2" trace=".default.xml"
    comment="Apres analyse morphologique, verifier que le mot feather a la propriete NC "
    left="XPATH#//data_structure/vertex[token/position=12]/data//property/p[@prop='MACRO']/@val"
    operator="contains"
    right="L_NC" />
  <test id="eng.disamb.1.3" trace=".disambiguated.xml"
    comment="Apres le PosTagger, verifier qu'il n'y a aucune succession prep verbe"
    left="XPATH#//vertices/vertex[macro='v']/pred/@id"
    operator="distinct"
    right="XPATH#//vertices/vertex[macro='prep']/@id" />
</testcase>
