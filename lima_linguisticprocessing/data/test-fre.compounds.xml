<testcases>
<testcase id="fre.cpd.1" type="bloquant">
  <call-parameters>
    <param key="text" value="Les oiseaux de type passereaux sont très petits."/>
    <param key="language" value="fre"/>
    <list key="pipelines">
      <item value="query"/>
      <item value="indexer"/>
    </list>
  </call-parameters>
  <expl>GN : Sub de Sub Sub : on doit obtenir le compound (*oiseau_(*type_passereau)).</expl>
    <test id="fre.cpd.1.0.a" trace=".disambiguated.xml"
    comment="'oiseaux' nom commun"
    left="XPATH#//vertices/vertex[@position=5]/macro"
    operator="="
    right="NC"
    conditional="yes"/>
    <test id="fre.cpd.1.0.b" trace=".disambiguated.xml"
    comment="'type' nom commun"
    left="XPATH#//vertices/vertex[@position=16]/macro"
    operator="contains"
    right="NC"
    conditional="yes"/>
    <test id="fre.cpd.1.0.c" trace=".disambiguated.xml"
    comment="'passereaux' nom commun"
    left="XPATH#//vertices/vertex[@position=21]/macro"
    operator="="
    right="NC"
    conditional="yes"/>
  <test id="fre.cpd.1.1" trace=".compounds.xml"
    comment="'type' est la tete de 'type_passereau' "
    left="XPATH#//compounds_dump/sentence/compounds/compound[@pos=5]/part/compound[@pos=16]/part[@head='true']/word[@form='type']"
    operator="exists"
    right="" />
  <test id="fre.cpd.1.2" trace=".compounds.xml"
    comment="'passereau' est l'extension de 'type_passereau' "
    left="XPATH#//compounds_dump/sentence/compounds/compound[@pos=5]/part/compound[@pos=16]/part[@head='false']/word[@form='passereau']"
    operator="exists"
    right="" />
  <test id="fre.cpd.1.3" trace=".compounds.xml"
    comment="'oiseau' est la tete de 'oiseau_(type_passereau)' "
    left="XPATH#//compounds_dump/sentence/compounds/compound[@pos=5]/part[@head='true']/word[@form='oiseau']"
    operator="exists"
    right="" />
  <test id="fre.cpd.1.4"  trace=".compounds.xml"
    comment="'type_passereau' est l'extension de 'oiseau_(type_passereau)' "
    left="XPATH#//compounds_dump/sentence/compounds/compound[@pos=5]/part[@head='false']/compound[@form='type_passereau']"
    operator="exists"
    right="" />
</testcase>
