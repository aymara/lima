      </list>
    </call-parameters>
    <test id="eng.approx.block.2.1" trace=".se.approx.xml"
          comment="2 instances with overlapping: 'B C' and 'C D' in the text. Only one ('B C') must be retained"
          left="XPATH#//specific_entities/specific_entity[position=3][length=3]/type"
          operator="contains"
          right="Total.BLOCK"/>
    <test id="eng.approx.block.2.2" trace=".se.approx.xml"
          comment="value of block name is 'B C'"
          left="XPATH#//specific_entities/specific_entity[position=3][length=3]/string"
