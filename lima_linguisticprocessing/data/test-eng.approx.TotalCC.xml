      <param key="meta" value="index:ForTest"/>
      <list key="pipelines">
        <item value="totalcruncher"/>
      </list>
    </call-parameters>
    <test id="eng.approx.block.1.1" trace=".se.approx.xml"
          comment="2 instances of name 'A' in the text. 'A' is in the dictionary block-countryNames.sorted.toy.lst for index ForTest)"
          left="XPATH#//specific_entities/specific_entity[position=8][length=1]/type"
          operator="contains"
          right="Total.BLOCK"/>
    <test id="eng.approx.block.1.2" trace=".se.approx.xml"
          comment="value of block name is 'A'"
          left="XPATH#//specific_entities/specific_entity[position=8][length=1]/string"
          operator="="
          right="A"/>
    <test id="eng.approx.block.1.3" trace=".se.approx.xml"
          comment="2 instances of name 'A' in the text. 'A' is in the dictionary block-countryNames.sorted.toy.lst for index ForTest)"
          left="XPATH#//specific_entities/specific_entity[position=3][length=1]/type"
          operator="contains"
