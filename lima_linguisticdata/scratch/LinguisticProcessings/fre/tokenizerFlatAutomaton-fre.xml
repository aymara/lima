        <event>c_m</event>
        <condition>
          <before>
            <event>c_M</event>
          </before>
          <before>
            <event>c_del</event>
<!--             <event>m_line</event> -->
          </before>
        </condition>
      </transition>
      <transition>
        <to_state>ALL_UPPER</to_state>
        <event>c_M</event>
        <condition>
          <before>
            <event>c_M</event>
          </before>
          <before>
            <event>c_del</event>
            <event>m_line</event>
          </before>
        </condition>
      </transition>
      <transition>
        <to_state>BLANK</to_state>
        <event>c_b</event>
        <setting>T_CARDINAL_ROMAN</setting>
        <default>t_cardinal_roman</default>
        <token/>
      </transition>
      <transition>
        <to_state>WORDITSELF</to_state>
        <event>c_del1</event>
        <event>c_comma</event>
        <event>c_slash</event>
        <event>c_hyphen</event>
        <event>c_quote</event>
        <event>c_percent</event>
        <event>c_fraction</event>
        <event>m_line</event>
        <setting>T_CARDINAL_ROMAN</setting>
        <default>t_cardinal_roman</default>
        <token/>
      </transition>
      <transition>
        <to_state>START</to_state>
        <event>all</event>
        <setting>T_NOT_ROMAN</setting>
      </transition>
    </state>
    <state id="ORDINAL_ROMAN">
      <transition>
        <to_state>ORDINAL_ROMAN</to_state>
        <event>c_o</event>
        <setting>T_ORDINAL_ROMAN</setting>
        <default>t_ordinal_roman</default>
      </transition>
      <transition>
        <to_state>START</to_state>
        <event>c_Mm</event>
        <setting>T_NOT_ROMAN</setting>
      </transition>
      <transition>
        <to_state>BLANK</to_state>
        <event>c_b</event>
        <setting>T_ORDINAL_ROMAN</setting>
        <default>t_ordinal_roman</default>
        <token/>
      </transition>
      <transition>
        <to_state>WORDITSELF</to_state>
        <event>c_del1</event>
        <event>c_comma</event>
        <event>c_slash</event>
        <event>c_hyphen</event>
        <event>c_quote</event>
        <event>c_percent</event>
        <event>c_fraction</event>
        <event>m_line</event>
        <setting>T_ORDINAL_ROMAN</setting>
        <default>t_ordinal_roman</default>
        <token/>
      </transition>
      <transition>
        <to_state>START</to_state>
        <event>all</event>
        <setting>T_NOT_ROMAN</setting>
      </transition>
    </state>
    <state id="START_NUMERIC">
      <transition>
        <to_state>INTEGER</to_state>
        <event>c_5</event>
        <setting>T_INTEGER</setting>
        <default>t_integer</default>
      </transition>
    </state>
    <state id="INTEGER">
      <transition>
        <to_state>INTEGER</to_state>
        <event>c_5</event>
      </transition>
      <transition>
        <to_state>COMMA_NUMBER</to_state>
        <event>c_comma</event>
        <condition>
          <after>
            <event>c_5</event>
          </after>
        </condition>
        <setting>T_COMMA_NUMBER</setting>
        <default>t_comma_number</default>
      </transition>
      <transition>
        <to_state>DOT_NUMBER</to_state>
        <event>c_dot</event>
        <condition>
          <after>
            <event>c_5</event>
          </after>
        </condition>
        <setting>T_DOT_NUMBER</setting>
        <default>t_dot_number</default>
      </transition>
      <transition>
        <to_state>FRACTION</to_state>
        <event>c_slash</event>
        <condition>
          <after>
            <event>c_5</event>
          </after>
        </condition>
        <setting>T_FRACTION</setting>
        <default>t_fraction</default>
      </transition>
      <transition>
        <to_state>ORDINAL_INTEGER</to_state>
        <event>c_o</event>
        <setting>T_ORDINAL_INTEGER</setting>
        <default>t_ordinal_integer</default>
      </transition>
      <transition>
        <to_state>WORDITSELF</to_state>
        <event>c_del1</event>
        <event>c_comma</event>
        <event>c_slash</event>
        <event>c_hyphen</event>
        <event>c_quote</event>
        <event>c_percent</event>
        <event>c_fraction</event>
        <event>c_dot</event>
        <event>m_line</event>
        <token/>
      </transition>
      <transition>
        <to_state>START</to_state>
        <event>c_b</event>
        <token/>
      </transition>
      <transition>
        <to_state>START</to_state>
        <event>all</event>
      </transition>
    </state>
    <state id="COMMA_NUMBER">
      <transition>
        <to_state>COMMA_NUMBER</to_state>
        <event>c_5</event>
      </transition>
      <transition>
        <to_state>COMMA_NUMBER</to_state>
        <event>all</event>
      </transition>
    </state>
    <state id="DOT_NUMBER">
      <transition>
        <to_state>DOT_NUMBER</to_state>
        <event>c_5</event>
      </transition>
      <transition>
        <to_state>DOT_NUMBER</to_state>
        <event>c_dot</event>
        <condition>
          <after>
            <event>c_5</event>
          </after>
        </condition>
      </transition>
      <transition>
        <to_state>DOT_NUMBER</to_state>
        <event>all</event>
      </transition>
    </state>
    <state id="FRACTION">
      <transition>
        <to_state>FRACTION</to_state>
        <event>c_5</event>
      </transition>
      <transition>
        <to_state>BLANK</to_state>
        <event>c_b</event>
        <token/>
      </transition>
      <transition>
        <to_state>WORDITSELF</to_state>
        <event>c_del1</event>
        <event>c_comma</event>
        <event>c_slash</event>
        <event>c_hyphen</event>
        <event>c_quote</event>
        <event>c_percent</event>
        <event>c_fraction</event>
        <event>m_line</event>
        <token/>
      </transition>
      <transition>
        <to_state>FRACTION</to_state>
        <event>all</event>
      </transition>
    </state>
    <state id="ORDINAL_INTEGER">
      <transition>
        <to_state>ORDINAL_INTEGER</to_state>
        <event>c_o</event>
      </transition>
      <transition>
        <to_state>ORDINAL_INTEGER</to_state>
        <event>all</event>
      </transition>
    </state>
<!--      <transition>
        <to_state>ALPHANUMERIC</to_state>
        <event>c_Mm</event>
        <condition>
          <status>s_not_numeric</status>
        </condition>
        
        <setting>T_ALPHANUMERIC</setting>
        <default>t_alphanumeric</default>
      </transition>-->
    <state id="PATTERN">
      <transition>
        <to_state>START</to_state>
        <event>m_end_pattern</event>
        <token/>
      </transition>
      <transition>
        <to_state>PATTERN</to_state>
        <event>all</event>
      </transition>
      <transition>
        <to_state>PATTERN</to_state>
        <event>unknwn</event>
      </transition>
      <transition>
        <to_state>END</to_state>
        <event>m_parag</event>
        <token/>
      </transition>
    </state>
    <state id="ALPHANUMERIC">
      <transition>
        <to_state>ALPHANUMERIC</to_state>
        <event>c_Mm</event>
        <event>c_5</event>
      </transition>
      <transition>
        <to_state>ALPHANUMERIC</to_state>
        <event>c_lowline</event>
      </transition>
      <transition>
        <to_state>ALPHANUMERIC</to_state>
        <event>unknwn</event>
      </transition>
      <transition>
        <to_state>START</to_state>
        <event>c_dot</event>
        <condition>
          <after>
            <event>c_b</event>
          </after>
          <after>
            <event>c_m</event>
          </after>
        </condition>
        <token/>
      </transition>
      <transition>
        <to_state>START</to_state>
        <event>all</event>
        <token/>
      </transition>
      <transition>
        <to_state>END</to_state>
        <event>m_parag</event>
        <token/>
      </transition>
    </state>
    <state id="END"/>
</language_definition>
